{
  "name": "Iggy",
  "address": "127.0.0.1:8080",
  "watcher": {
    "enabled": true,
    "interval": 30
  },
  "system": {
    "path": "local_data",
    "stream": {
      "path": "streams",
      "topic": {
        "path": "topics",
        "partition": {
          "segment": {
            "size_bytes": 1000000,
            "messages_required_to_save": 10,
            "messages_buffer": 10
          }
        }
      }
    }
  }
}